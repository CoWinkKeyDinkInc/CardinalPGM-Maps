<map proto="1.3.2">
<name>Deepwind Jungle</name>
<version>1.2.1</version>
<objective>
Retrieve the wool from the enemy's side and place it on your team's monument
</objective>
<authors>
<author uuid="91295196-9cd2-4963-b7ec-afd1a110edbe"/>
<!--  PandaBattalion  -->
</authors>
<contributors>
<contributor uuid="41373965-b60a-40d5-8fad-72e61d951ea2" contribution="Aesthetics and spawn kit help"/>
<!--  Rokosnocone  -->
</contributors>
<teams>
<team color="blue" max="16" max-overfill="20">Blue</team>
<team color="dark red" max="16" max-overfill="20">Red</team>
</teams>
<spawns>
<spawns kit="blue">
<spawn team="blue" kit="blue" yaw="0">
<cuboid min="-1,9,-76" max="1,9,-73"/>
</spawn>
</spawns>
<spawns kit="red">
<spawn team="red" kit="red" yaw="180">
<cuboid min="-1,9,73" max="1,9,76"/>
</spawn>
</spawns>
<default yaw="90">
<cylinder base="-50,59,0" radius="3" height="0"/>
</default>
</spawns>
<kits>
<kit name="spawn">
<item slot="0">stone sword</item>
<item slot="1">bow</item>
<item slot="2" enchantment="durability:2;dig speed:1">iron pickaxe</item>
<item slot="3" enchantment="durability:1">iron spade</item>
<item slot="4" enchantment="durability:2;dig speed:1">stone axe</item>
<item slot="5" amount="32">cooked fish</item>
<item slot="6" amount="1">golden apple</item>
<item slot="7" amount="64">glass</item>
<item slot="8" amount="32" damage="3">log</item>
<item slot="28" amount="32">arrow</item>
<potion duration="10" amplifier="3">damage resistance</potion>
</kit>
<kit name="red" parents="spawn">
<helmet color="db1818" damage="-3000">leather helmet</helmet>
<chestplate color="db1818" enchantment="protection projectile:2" damage="-3000">leather chestplate</chestplate>
<leggings color="db1818" damage="-3000">leather leggings</leggings>
<boots color="db1818" damage="-3000">leather boots</boots>
</kit>
<kit name="blue" parents="spawn">
<helmet color="3c1cd9" damage="-3000">leather helmet</helmet>
<chestplate color="3c1cd9" enchantment="protection projectile:2" damage="-3000">leather chestplate</chestplate>
<leggings color="3c1cd9" damage="-3000">leather leggings</leggings>
<boots color="3c1cd9" damage="-3000">leather boots</boots>
</kit>
</kits>
<filters>
<filter name="only-blue">
<team>blue</team>
</filter>
<filter name="only-red">
<team>red</team>
</filter>
<filter name="no-void">
<deny>
<void/>
</deny>
</filter>
<!--  Special Filters  -->
<filter name="allow-some" parents="deny-all">
<allow>
<block>glass</block>
<block>fence</block>
<block damage="9">wool</block>
<block damage="13">wool</block>
<block damage="10">wool</block>
<block damage="4">wool</block>
</allow>
</filter>
<filter name="only-blue-sp">
<all>
<team>blue</team>
<filter name="allow-some"/>
</all>
</filter>
<filter name="only-red-sp">
<all>
<team>red</team>
<filter name="allow-some"/>
</all>
</filter>
<filter name="only-red-usage">
<not>
<all>
<any>
<block>chest</block>
<block>workbench</block>
</any>
<team>blue</team>
</all>
</not>
</filter>
<filter name="only-blue-usage">
<not>
<all>
<any>
<block>chest</block>
<block>workbench</block>
</any>
<team>red</team>
</all>
</not>
</filter>
</filters>
<regions>
<union name="blue-wool-rooms">
<rectangle name="green-room" min="17,66" max="37,79"/>
<rectangle name="cyan-room" min="-17,66" max="-37,79"/>
</union>
<apply block-break="only-blue-sp" block-place="deny-all" use="only-blue-usage">
<region name="blue-wool-rooms"/>
</apply>
<apply enter="only-blue" message="You may not enter your own wool room!">
<region name="blue-wool-rooms"/>
</apply>
<union name="red-wool-rooms">
<rectangle name="purple-room" min="-17,-65" max="-37,-78"/>
<rectangle name="yellow-room" min="17,-65" max="37,-78"/>
</union>
<apply block-break="only-red-sp" block-place="deny-all" use="only-red-usage">
<region name="red-wool-rooms"/>
</apply>
<apply enter="only-red" message="You may not enter your own wool room!">
<region name="red-wool-rooms"/>
</apply>
<apply block-place="no-void" message="You may not place blocks in the void!">
<complement>
<negative>
<rectangle min="-31,46" max="31,-46"/>
</negative>
<region name="red-wool-rooms"/>
<region name="blue-wool-rooms"/>
</complement>
</apply>
<!--     spawn protection     -->
<apply block="deny-all" message="You may not break or place blocks in your spawn!">
<rectangle name="blue-spawn" min="-7,-55" max="7,-82"/>
<rectangle name="red-spawn" min="-7,56" max="7,83"/>
</apply>
<!--     prevent teams from entering each other's spawns     -->
<apply enter="only-blue" message="You cannot enter the other team's spawn!">
<region name="blue-spawn"/>
</apply>
<apply enter="only-red" message="You cannnot enter the other team's spawn!">
<region name="red-spawn"/>
</apply>
</regions>
<toolrepair>
<tool>stone sword</tool>
<tool>iron spade</tool>
<tool>iron pickaxe</tool>
<tool>bow</tool>
<tool>stone axe</tool>
</toolrepair>
<itemremove>
<item>golden apple</item>
<item>glass</item>
<item>leather helmet</item>
<item>leather chestplate</item>
<item>leather leggings</item>
<item>leather boots</item>
<item>fence</item>
<item>string</item>
<item>chainmail helmet</item>
<item>gold boots</item>
<item>water lily</item>
<item>bucket</item>
<item>water bucket</item>
<item>cocoa</item>
<item>cooked fish</item>
<item>5:3</item>
</itemremove>
<playable>
<rectangle min="-41,-82" max="41,83"/>
</playable>
<maxbuildheight>32</maxbuildheight>
<wools>
<wool team="blue" color="cyan">
<block location="-2,10,-81"/>
</wool>
<wool team="blue" color="green">
<block location="1,10,-81"/>
</wool>
<wool team="red" color="purple">
<block location="-2,10,81"/>
</wool>
<wool team="red" color="yellow">
<block location="1,10,81"/>
</wool>
</wools>
</map>