<?xml version="1.0"?>
<map proto="1.3.6">
<name>Infinite</name>
<version>1.0</version>
<objective>Eliminate your opponents!</objective>
<authors>
    <author uuid="e79fbdc9-627e-4dac-97a6-81a3046220d7"/>
    <!--AtlasGames-->
</authors>
<contributors>
    <contributor contribution="XML" uuid="bb628049-e368-4b1c-b8aa-d4557ae9416b"/>
    <!--Anthonny-->
</contributors>
<blitz>
    <time>5m</time>
    <lives>2</lives>
    <title>Blitz: Rage</title>
</blitz>
<incude src="blitz-global.xml"/>
<rage/>
<teams>
    <team color="dark red" max="10" max-overfill="12">Red Team</team>
    <team color="blue" max="10" max-overfill="12">Blue Team</team>
</teams>
<kits>
    <kit name="spawn">
        <item slot="0" lore="1 Hit Kill!" enchantment="damage all:10">iron sword</item>
        <item slot="1" lore="1 Shot Kill!" enchantment="arrow damage:10">bow</item>
        <item slot="2">arrow</item>
        <item slot="8" amount="2">ender pearl</item>
        <chestplate>iron chestplate</chestplate>
        <potion duration="oo" amplifier="2" ambient="true">speed</potion>
        <potion duration="oo" amplifier="2" ambient="true">jump</potion>
    </kit>
    <kit name="red" parents="spawn">
        <helmet color="aa0000" locked="true">leather helmet</helmet>
        <leggings color="aa0000" locked="true">leather leggings</leggings>
        <boots color="aa0000" locked="true">leather boots</boots>
    </kit>
    <kit name="blue" parents="spawn">
        <helmet color="0000aa" locked="true">leather helmet</helmet>
        <leggings color="0000aa" locked="true">leather leggings</leggings>
        <boots color="0000aa" locked="true">leather boots</boots>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="red">
        <regions yaw="135">
            <block>29.5,20,95.5</block>
        </regions>
    </spawn>
    <spawn team="blue" kit="blue">
        <regions yaw="-45">
            <block>-37.5,19,-11.5</block>
        </regions>
    </spawn>
    <default>
        <regions yaw="0">
            <cuboid min="1,59,-8.5" max="1,59,-8.5"/>
        </regions>
    </default>
</spawns>
<regions>
    <rectangle name="map" min="-oo,-oo" max="oo,oo"/>
    <apply block="deny-all">
        <region name="map"/>
    </apply>
</regions>
<killreward>
    <item>arrow</item>
</killreward>
<itemremove>
    <item>iron sword</item>
    <item>bow</item>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>iron chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>ender pearl</item>
</itemremove>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
</map>