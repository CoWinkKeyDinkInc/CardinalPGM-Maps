<map proto="1.4.0">
<name>Table Block</name>
<version>1.0.0</version>
<objective>Break into the enemy's monuments and bring back the wools that are inside.</objective>
<!--  Map authors & contributors.  -->
<authors>
    <author uuid="bbcafea0-53cf-4d29-9dd9-27f383d02faa" contribution="Map Builder"/><!--Spintown-->
    <author uuid="7cbf2168-7f49-4fb3-aea8-6df8048a7ade" contribution="XML"/><!--nigges60-->
</authors>
<contributors>
    <contributor uuid="3518f310-17e9-4d7c-b383-7735aeeba65a" contribution="MCEdit"/><!--Lmac77-->
    <contributor uuid="2fa0ea62-ca7f-477c-95d0-bff5be0f65b4" contribution="WorldEdit"/><!--8BitHer0-->
    <contributor uuid="bb628049-e368-4b1c-b8aa-d4557ae9416b" contribution="Provided Feedback"/><!--Anthonny-->
</contributors>
<gamemode>mixed</gamemode>
<!--Teams-->
<teams>
    <team id="yellow-team" color="yellow" max="32" max-overfill="40">Yellow</team>
    <team id="blue-team" color="blue" max="32" max-overfill="40">Blue</team>
</teams>
<!--Spawns-->
<spawns>
    <spawn team="yellow-team" kit="yellow-kit">
        <regions yaw="-90">
            <circle center="-476,14,-411" radius="1.5"/>
        </regions>
    </spawn>
    <spawn team="blue-team" kit="blue-kit">
        <regions yaw="90">
            <circle center="-138,14,-408" radius="1.5"/>
        </regions>
    </spawn>
    <default>
        <regions>
            <cuboid min="-312,50.5,-483" max="-306,51,-477"/>
        </regions>
    </default>
</spawns>
<!--Kits-->
<kits>
    <kit id="spawn">
        <item slot="0" material="stick"/>
        <item slot="1" unbreakable="true" material="bow">
            <enchantments>
                <enchantment level="5">sharpness</enchantment>
                <enchantment level="1">infinity</enchantment>
            </enchantments>
        </item>
        <item slot="2" unbreakable="true" material="diamond pickaxe"/>
        <item slot="3" unbreakable="true" material="iron axe"/>
        <item slot="5" amount="32" damage="2" material="wood"/>
        <item slot="7" amount="64" material="cooked beef"/>
        <item slot="8" amount="2"  material="golden apple"/>
        <item slot="17" amount="1" material="arrow"/>
        <potion amplifier="100" duration="3">heal</potion>
        <potion amplifier="100" duration="3">damage resistance</potion>
        <potion amplifier="100" duration="3">regeneration</potion>
    </kit>
    <kit id="yellow-kit" parents="spawn">
        <item slot="6" amount="64" damage="4" material="stained clay"/>
        <helmet color="E5E533" material="leather helmet"/>
        <chestplate color="E5E533" material="leather chestplate"/>
        <leggings color="E5E533" material="leather leggings"/>
        <boots color="E5E533" material="leather boots"/>
    </kit>
    <kit id="blue-kit" parents="spawn">
        <item slot="6" amount="64" damage="3" material="stained clay"/>
        <helmet color="334CB2" material="leather helmet"/>
        <chestplate color="334CB2" material="leather chestplate"/>
        <leggings color="334CB2" material="leather leggings"/>
        <boots color="334CB2" material="leather boots"/>
    </kit>
</kits>
<!--Filters-->
<filters>
    <team id="only-yellow">yellow-team</team>
    <team id="only-blue">blue-team</team>
    <not id="deny-void">
        <void/>
    </not>
    <all id="yellowwoolrooms">
        <team>yellow-team</team>
    </all>
    <all id="bluewoolrooms">
        <team>blue-team</team>
    </all>
    <deny id="no-safeties">
        <any>
            <material>wool</material>
        </any>
    </deny>
</filters>
<!--Regions-->
<regions>
    <rectangle id="bluespawn" min="-142,-416" max="-128,-402"/>
    <rectangle id="bluegap" min="-186,-446" max="-182,-372"/>
    <rectangle id="midgap" min="-328,-446" max="-289,-372"/>
    <rectangle id="yellowgap" min="-435,-446" max="-431,-372"/>
    <rectangle id="yellowspawn" min="-489,-416" max="-475,-402"/>
    <union id="yellowmonus">
        <cuboid id="magenta" min="-452,11,-389" max="-446,11,-383"/>
        <cuboid id="brown" min="-452,11,-436" max="-446,11,-430"/>
    </union>
    <union id="bluemonus">
        <cuboid id="silver" min="-171,11,-435" max="-165,11,-429"/>
        <cuboid id="gray" min="-171,11,-388" max="-165,11,-382"/>
    </union>
    <union id="woolindicator">
        <cuboid min="-169,45,-386" max="-167,48,-384"/>
        <cuboid min="-169,45,-433" max="-167,48,-431"/>
        <cuboid min="-450,45,-434" max="-448,48,-432"/>
        <cuboid min="-450,45,-387" max="-448,48,-385"/>
    </union>
    <union id="ywoolrooms">
        <union id="ywoolrooms"><!--Wools yellow need to get-->
            <cuboid id="silverwool" min="-171,0,-435" max="-165,11,-429"/>
            <cuboid id="graywool" min="-171,0,-388" max="-165,11,-382"/>
        </union>
        <union id="bwoolrooms"><!--Wools blue need to get-->
            <cuboid id="magentawool" min="-452,0,-389" max="-446,11,-38"/>
            <cuboid id="brownwool" min="-452,0,-436" max="-446,11,-430"/>
        </union>
    </union>
    <union id="iron">
        <cuboid min="-487,12,-386" max="-486-15,-373"/>
        <cuboid min="-131,12,-445" max="-130,15,-432"/>
    </union>
    <negative id="yes-void">
        <rectangle id="PlayArea" min="-492,-446" max="-125,-372"/>
    </negative>
        <circle id="yellowclock" center="-449,-409" radius="5"/><!--Yellowclock-->
        <circle id="blueclock" center="-168,-409" radius="5"/><!--Blueclock-->
<!--Applying-->
    <apply enter="only-yellow" block="yellowwoolrooms" use="only-yellow" region="ywoolrooms" message="You may not enter your wool rooms!"/>
    <apply enter="only-blue" block="bluewoolrooms" use="only-blue" region="bwoolrooms" message="You may not enter your wool rooms!"/>
    <apply enter="only-yellow" region="yellowspawn" message="You may not enter the enemy spawn!"/>
    <apply enter="only-blue" region="bluespawn" message="You may not enter the enemy spawn!"/>
    <apply block="deny-void" region="yes-void" message="You may not build in the void!"/>
    <apply blockplace="no-safeties" region="PlayArea" message="You may not place safeties!"/>
    <apply block-break="never" region="woolindicator" message="You may not break this!"/>
    <apply enter="only-yellow" region="blueclock" message="You may not enter this!"/>
    <apply enter="only-blue" region="yellowclock" message="You may not enter this!"/>
</regions>
<!--Portals-->
<portals>
    <portal x="@-248" y="@16" z="@-433" filter="only-blue">
        <region id="bluemonus"/>
	</portal>
    <portal x="@-369" y="@16" z="@-385" filter="only-yellow">
        <region id="yellowmonus"/>
	</portal>
    <portal x="@-169" y="@19" z="@-409">
        <region>
            <cuboid min="-169,2,-429" max="-167,4,-429"/><!--Silver-->
        </region>
    </portal>
    <portal x="@-169" y="@19" z="@-409">
        <region>
            <cuboid min="-169,2,-382" max="-167,4,-382"/><!--Gray-->
        </region>
    </portal>
    <portal x="@-448" y="@19" z="@-409">
        <region>
            <cuboid min="-450,2,-389" max="-448,4,-389"/><!--Magenta-->
        </region>
    </portal>
    <portal x="@-448" y="@19" z="@-409">
        <region>
            <cuboid min="-450,2,-436" max="-448,4,-436"/><!--Brown-->
        </region>
    </portal>
</portals>
<!--Wools-->
<wools team="yellow-team">
    <wool color="gray" location="-168,1,-386" craftable="false">
        <monument><block>-478,14,-414</block></monument>
    </wool>
    <wool color="silver" location="-168,1,-433" craftable="false">
        <monument><block>-480,14,-414</block></monument>
    </wool>
</wools>
<wools team="blue-team">
    <wool color="magenta" location="-449,1,-385" craftable="false">
        <monument><block>-137,14,-404</block></monument>
    </wool>
    <wool color="brown" location="-449,1,-432" craftable="false">
        <monument><block>-139,14,-404</block></monument>
    </wool>
</wools>
<!--Monuments-->
<destroyables materials="obsidian" mode-changes="true">
    <destroyable name="Silver Entrance" owner="blue-team" completion="1%">
        <region id="silver"/>
    </destroyable>
    <destroyable name="Gray Entrance" owner="blue-team" completion="1%">
        <region id="gray"/>
    </destroyable>
    <destroyable name="Magenta Entrance" owner="yellow-team" completion="1%">
        <region id="magenta"/>
    </destroyable>
    <destroyable name="Brown Entrance" owner="yellow-team" completion="1%">
        <region id="brown"/>
    </destroyable>
</destroyables>
<modes>
    <mode after="15m" material="gold block"/>
    <mode after="30m" material="coal block"/>
    <mode after="45m" material="glass"/>
</modes>
<renewables>
    <renewable rate="1" grow="false" particle="true" avoid-enteties="true" region="iron">
        <renew-filter>
            <material>iron block</material>
        </renew-filter>
    </renewable>
</renewables>
<itemremove>
    <item>diamond pickaxe</item>
    <item>iron axe</item>
    <item>bow</item>
    <item>cooked beef</item>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<maxbuildheight>46</maxbuildheight>
</map>
